{
  "rewrites": [
    {
      "source": "/route/:path*",
      "destination": "https://a2climbing.kro.kr/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://a2-c.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "/",
      "statusCode": 302
    }
  ]
}
